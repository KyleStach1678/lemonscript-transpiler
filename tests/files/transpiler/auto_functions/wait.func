Wait(Time wait_time)

include {
  "muan/utils/timing_utils.h"
}

globals {
  Time start_time
}

init {
  // auto start_time = lemon_var("start_time", muan::now());
  start_time = muan::now();
  return false;
}

periodic {
  Time start_time = 0 * s;
  return (muan::now() - start_time < wait_time);
}
